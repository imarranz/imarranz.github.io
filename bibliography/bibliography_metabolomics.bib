	journal = {PloS one},
	author = {Karpievitch, Yuliya V. and Nikolic, Sonja B. and Wilson, Richard and Sharman, James E. and Edwards, Lindsay M.},
	year = {2014},
	pmid = {25549083},
	pmcid = {PMC4280143},
	keywords = {Aged, Chromatography, Liquid/methods, Databases, Factual, Diabetes Mellitus, Type 2/*blood/*metabolism, Humans, Mass Spectrometry/methods, Metabolomics/*methods, Middle Aged, Reproducibility of Results, Software},
