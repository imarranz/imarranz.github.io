@incollection{miroslava2013,
title = {5 - Metabolomics data analysis – processing and analysis of a dataset},
editor = {Čuperlović-Culf, Miroslava},
booktitle = {\{NMR\} Metabolomics in Cancer Research},
publisher = {Woodhead Publishing},
year =  {2013},
pages = {261--333},
series = {Woodhead Publishing Series in Biomedicine},
isbn = {978-1-907568-84-8},
doi = {http://dx.doi.org/10.1533/9781908818263.261},
url = {http://www.sciencedirect.com/science/article/pii/B9781907568848500056},
author = {Miroslava Čuperlović-Culf},
keywords = {normalization and scaling in metabolomics},
keywords = {confounders},
keywords = {unsupervised data analysis},
keywords = {supervised data analysis},
keywords = {clustering},
keywords = {classification},
keywords = {visualization methods},
keywords = {data integration},
abstract = {Abstract: Differences in sample quantities, as well as experimental variations, require the application of an optimal normalization procedure. Variations in the metabolite profiles caused by factors beyond an investigator’s control and interest may also affect the data, and lead to erroneous and incorrect conclusions. Corrections for these factors must be made, in addition to spectral preprocessing and/or metabolite quantification. Subsequently, it is possible to perform different types of analysis, ranging from unsupervised analyses (e.g. to determine sample sub-types or metabolite grouping) to supervised analyses aimed at classification and feature selection. Likewise, quantified metabolic data can be integrated with the results from other experiments, e.g. transcriptomics or proteomics, leading to the development of systems biology models. Data analysis strategy depends on specific study and there is no approach that can fit all data or all questions. This chapter provides an overview of methods used in metabolomics data analysis with their positive and negative sides and many examples.}
}

@article{karpievitch2014,
	title = {Metabolomics data normalization with {EigenMS}.},
	volume = {9},
