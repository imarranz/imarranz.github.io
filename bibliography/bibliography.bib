@Article{Folger2011,
  Title                    = {Predicting selective drug targets in cancer through metabolic networks.},
  Author                   = {Folger, Ori and Jerby, Livnat and Frezza, Christian and Gottlieb, Eyal and Ruppin, Eytan and Shlomi, Tomer},
