@Article{Halama2013,
  Title                    = {Identification of biomarkers for apoptosis in cancer cell lines using metabolomics: tools for individualized medicine.},
  Author                   = {Halama, A. and Riesen, N. and MÃÂ¶ller, G. and {HrabÃÂ de Angelis}, M. and Adamski, J.},
