  File                     = {:Jabref Catalogado\\2013.Halama_Apoptosis.pdf:PDF},
  Institution              = {Helmholtz Zentrum MÃÂ¼nchen, German Research Center for Environmental Health, Institute of Experimental Genetics, Genome Analysis Center, Neuherberg, Germany.},
