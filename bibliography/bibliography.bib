@Article{Scalbert2009,
  Title                    = {Mass-spectrometry-based metabolomics: limitations and recommendations for future progress with particular focus on nutrition research.},
  Author                   = {Scalbert, Augustin and Brennan, Lorraine and Fiehn, Oliver and Hankemeier, Thomas and Kristal, Bruce S. and {van Ommen}, Ben and Pujos-Guillot, Estelle and Verheij, Elwin and Wishart, David and Wopereis, Suzan},
