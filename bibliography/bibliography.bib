  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {ibon},
  Pmid                     = {19754161},
  Timestamp                = {2012.12.19},
  Url                      = {http://dx.doi.org/10.1021/pr900499r}
}

