  File                     = {:Jabref Catalogado\\2012.Holmes_NatureMethods_Imperial.pdf:PDF},
  Institution              = {Biomolecular Medicine, Department of Surgery and Cancer, Faculty of Medicine, Imperial College, London, UK.},
  Language                 = {eng},
