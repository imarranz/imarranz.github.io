@Article{Chiarugi2012,
  Title                    = {The NAD metabolome--a key determinant of cancer cell biology.},
  Author                   = {Chiarugi, Alberto and DÃÂ¶lle, Christian and Felici, Roberta and Ziegler, Mathias},
  Journal                  = {Nat Rev Cancer},
